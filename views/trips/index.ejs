<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th>Date</th>
      <th>Destination</th>
      <th>Activities</th>
    </tr>
  </thead>
  <tbody>
    <% trips.forEach(function(t) { %>
      <tr>
        <td><%= t.date %></td>
        <td><%= t.location %></td>
        <!-- <td><%= t.activity %></td> -->
        <td><a href="/trips/<%= t._id %>">Trip Itinerary</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
